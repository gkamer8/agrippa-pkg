<model script-version="0.0.1">

    <import dim="[3, 1]" from="input" type="float32" />

    <block title="Down Projection">
        <import from="input" />
        <node title="Linear" op="MatMul">
            <params dim="[2, 3]" name="W" type="float32" shared="no" />
            <input dim="[3, 1]" src="input" />
            <output dim="[2, 1]" name="down" />
        </node>
        <export from="down" />
    </block>

    <block title="Up Projection">
        <import from="down" />
        <node title="Linear" op="MatMul">
            <params dim="[3, 2]" name="WT" type="float32" shared="no" />
            <input dim="[2, 1]" src="down" />
            <output dim="[3, 1]" name="linear2" />
        </node>
        <export from="linear2" />
    </block>

    <block title="Add">
        <import from="linear2" />
        <node title="Bias" op="Add">
            <params dim="[3, 1]" name="b" type="float32" />
            <input dim="[3, 1]" src="linear2" />
            <output dim="[3, 1]" name="biased" />
        </node>
        <export from="biased" />
    </block>

    <block title="Activation">
        <import from="biased" />
        <node title="ReLu" op="Relu">
            <input dim="[3, 1]" src="biased" />
            <output dim="[3, 1]" name="y" />
        </node>
        <export from="y" />
    </block>

    <export dim="[3, 1]" from="y" type="float32" />
    <export dim="[2, 1]" from="down" type="float32" />

</model>